  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter frontend",
      "type": "dart",
      "request": "launch",
      "program": "frontend/lib/main.dart"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'vestibule'",
      "cargo": {
        "args": ["build", "--bin=vestibule", "--package=vestibule"],
        "filter": {
          "name": "vestibule",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'vestibule'",
      "cargo": {
        "args": ["test", "--no-run", "--bin=vestibule", "--package=vestibule"],
        "filter": {
          "name": "vestibule",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
}
